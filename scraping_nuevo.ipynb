    "from selenium import webdriver\n",
    "from selenium.webdriver.common.by import By\n",
    "from selenium.webdriver.support.ui import WebDriverWait\n",
    "from selenium.webdriver.support import expected_conditions as EC\n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "URL_BASE = \"https://www.mwcbarcelona.com/exhibitors\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 126,
   "id": "f1ab4198",
   "metadata": {},
   "outputs": [],
   "source": [
    "def cookie_handler(driver, path):\n",
    "    button = WebDriverWait(driver, 5).until(EC.element_to_be_clickable((By.XPATH, path)))\n",
    "    button.click()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 127,
   "id": "0a8beabe",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get(driver, path):\n",
    "    return WebDriverWait(driver, 5).until(EC.presence_of_element_located((By.XPATH, path)))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 128,
   "id": "76042498",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_info(driver):\n",
    "    path = [\n",
    "        '/html/body/header/div/div[2]/header/div/div[1]/div[1]/h1',  # name\n",
    "        '/html/body/div[1]/main/p',  # info\n",
    "        '/html/body/div[1]/aside/div/ul',  # location\n",
    "        '/html/body/div[1]/aside/div[2]',  # tags\n",
    "        '//li[contains(@class, \"flex w-full md:w-1/3 pr-2\")]//a[@target=\"_blank\"]',  # web\n",
    "        '/html/body/header/div/div[2]/header/div/div[2]'  # social\n",
    "    ]\n",
    "    result = []\n",
    "    for i in path:\n",
    "        try:\n",
    "            texto = get(driver, i)\n",
    "            if i != path[4] or i != path[5]:\n",
    "                result.append(texto.text)\n",
    "            else:\n",
    "                result.append(texto.get_attribute('href'))\n",
    "        except:\n",
    "            result.append(None)\n",
    "   
    "    return {\n",
    "        'Company': result[0],\n",
    "        'About': result[1],\n",
    "        'Location': result[2],\n",
    "        'Interests': result[3],\n",
    "        'Web_page': result[4],\n",
    "        'Social_media': result[5]\n",
    "    }\n",
    "def getting_subs(driver):\n",
    "    pages = WebDriverWait(driver, 20).until(EC.presence_of_all_elements_located((By.XPATH, '//tr[@role=\"link\"]')))\n",
    "    links = [i.get_attribute(\"data-href\") for i in pages]\n",
    "    return links"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 129,
   "id": "3a77e271",
   "metadata": {},
   "outputs": [],
   "source": [
    "pages = [\n",
    "    'https://www.mwcbarcelona.com/exhibitors',\n",
    "    'https://www.mwcbarcelona.com/exhibitors?page=2'\n",
    "]\n",
    "\n",
    "data = []\n",
    "driver = webdriver.Chrome()\n",
    "driver.maximize_window()\n",
    "\n",
    "for sub_page in pages:\n",
    "    driver.get(sub_page)\n",
    "    driver.implicitly_wait(0.5)\n",
    "    if sub_page == pages[0]:\n",
    "        cookie, cruz = '//*[@id=\"onetrust-accept-btn-handler\"]', '//*[@id=\"register-interest-bottom-bar\"]/span/a'\n",
    "        cookie_handler(driver, cookie)\n",
    "        cookie_handler(driver, cruz)\n",
    "\n",
    "    links = getting_subs(driver)\n",
    "    driver.execute_script(\"window.open(\"\");\") \n",
    "    driver.switch_to.window(driver.window_handles[1])\n",
    "    for i in links:\n",
    "        driver.get('https://www.mwcbarcelona.com' + str(i))\n",
    "        driver.implicitly_wait(0.5)\n",
    "        data.append(get_info(driver))"
   ]
  },
